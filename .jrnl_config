{
  "journals": {
    "default": {
      "journal": "$HOME/Dropbox/Sync/journal.txt",
      "encrypt": false
    }
  },
  "editor": "vim -c 'set wrap linebreak'",
  "encrypt": false,
  "default_hour": 0,
  "default_minute": 0,
  "timeformat": "%Y-%m-%d %H:%M",
  "tagsymbols": "@",
  "highlight": true,
  "linewrap": 100,
  "prettytimeformat": "%A, %b %d %Y",
  "entry_separator": "\n\n"
}
